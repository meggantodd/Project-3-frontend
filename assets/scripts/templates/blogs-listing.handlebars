{{#each blogs as |blog|}}

<div id="all-blogs-container" class="col-xs-4 box">
    <div id="all-blogs-container" data-id="{{blog.id}}">
      <ul name='blog-name' class='handlebars-fields'>Title: <span class='handlebars' id='blog-title'>{{blog.title}}</span></ul>


    {{#checkUser blog._owner}}

      <button type="submit" class="remove-button" name="remove-button">Remove</button>
      <button type="submit" class="edit-button" name="edit-button">Edit</button>

    {{/checkUser}}
  </div>
</div>



{{/each}}
